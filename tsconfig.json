{
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "lib": [
      "es5",
      "dom"
    ],
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "rootDirs": [
      "src",
      "dist"
    ]
  },
  "files": [
    "src/index.ts",
  ]
}